<UserControl x:Class="CPSC_481_Trailexplorers.HikeListPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CPSC_481_Trailexplorers"
             mc:Ignorable="d"
             Height="768" Width="432"
            >
    <Grid Background="White">
        <Image HorizontalAlignment="Left" Height="46" Margin="0,10,0,0" VerticalAlignment="Top" Width="46" Source="Assets/backArrow.png" MouseDown="Back_MouseDown"/>
        <Image x:Name="logoImage" Panel.ZIndex="3" 
            Source="Logos/logoWBold.png" RenderOptions.BitmapScalingMode="Fant" Opacity="1" Margin="108,8,108,562" Stretch="UniformToFill"/>
        <Image x:Name="backGroundTop" HorizontalAlignment="Left" Margin="0,61,0,551" Width="432" Source="BackgroundImages/trail.png" Stretch="UniformToFill" >
            <Image.Effect>
                <BlurEffect/>
            </Image.Effect>
        </Image>

    </Grid>
</UserControl>
